[color]
    ui = true
    
[push]
    default = current

[alias]
    st = status
    ci = commit
    co = checkout
    br = branch
    fe = fetch
    re = rebase
    last = log --stat -1
    list = log --pretty=full
    f-fs = flow feature start
    f-ff = flow feature finish