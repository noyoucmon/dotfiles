[include]
    path = ~/.gituser

[color]
    ui = true
    
[push]
    default = current

[alias]
    st = status
    ci = commit
    co = checkout
    br = branch
    fe = fetch
    re = rebase
    last = log --stat -1
    list = log --pretty=full
    gf-fs = flow feature start
    gf-ff = flow feature finish